package Things;

public abstract class Creature extends Thing{
	
	private Thing inBelly;
	public Creature(){
		super();
		inBelly=null;
	}
	public void eat(Thing aThing){
		System.out.println();
		setInBelly(aThing);
	}
	public abstract void move();
	public void whatDidYouEat(){
		
	}
	private void setInBelly(Thing aThing){
		inBelly=aThing;
	}
}
